#!/bin/bash
awk -F "\"*,\"*" '{print $2}' /tmp/MIHIR/ScheduledReportNRMAMonthlyPatchReportOracleCloud-2020-09-21030104.csv >  /tmp/MIHIR/column.txt
st="$(grep -n "DNS" column.txt | head -n 1 | cut -d: -f1)"
st3="$(($st +1))"
st2="$(grep -n "QID" column.txt | head -n 1 | cut -d: -f1)"
st4="$(($st2 - 4))"
sed -n "$st3","$st4"p column.txt > serverlist2.txt
